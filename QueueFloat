package TUGAS_ASD3;



import java.util.ArrayList;



public class QueueFloat {

	ArrayList<Float> kelase = new ArrayList<Float>();

	int ismail = -1;



	public void insert(Float value) {

		inda++;

		kelase.add(inda,value);

	}

	public Float get() {

		Float value = 0.1f;

		if(inda>-1) {

			value = kelase.get(0);

			kelase.remove(0);

			inda--;

		}

		return value;

		

	}

	public void cetak() {

		System.out.println("ECOR : "+inda+" > VARIABLE: "+kelase.toString());

	}

}
